{
    "name": "cobie-excel-connector",
    "version": "1.0.0",
    "description": "COBie Excel Connector (powered by pcf)",
    "scripts": {
        "start": "tsc && node ./lib/App.js",
        "build": "tsc && npm run copyFiles",
