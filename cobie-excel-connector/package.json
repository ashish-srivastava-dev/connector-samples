        "clean": "rimraf lib"
    },
    "main": "./lib/App.js",
    "license": "MIT",
    "dependencies": {
        "@bentley/bentleyjs-core": "^2.16.0",
        "@bentley/ecschema-metadata": "^2.16.0",
        "@bentley/electron-manager": "^2.16.0",
        "@bentley/imodeljs-backend": "^2.16.0",
        "@bentley/imodeljs-common": "^2.16.0"
    },
    "devDependencies": {
        "@bentley/build-tools": "^2.16.0",
        "@types/node": "^10.17.28",
        "babel-cli": "^6.26.0",
        "typescript": "^4.1.0"
    }
}
